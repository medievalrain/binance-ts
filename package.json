{
	"name": "@medievalrain/binance-ts",
	"version": "0.10.0",
	"description": "Binance API SDK",
	"access": "public",
	"type": "module",
	"license": "MIT",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"gen:all": "NODE_OPTIONS='--localstorage-file=ls' ts-to-zod --all",
		"lint": "oxlint",
		"format": "prettier --check .",
		"typecheck": "tsc"
	},
	"devDependencies": {
		"@types/node": "24.9.1",
		"date-fns": "4.1.0",
		"dotenv": "17.2.3",
		"oxlint": "1.24.0",
		"prettier": "3.6.2",
		"ts-to-zod": "5.0.1",
		"tsdown": "0.15.10",
		"typescript": "5.9.3",
		"vitest": "4.0.4",
		"zod": "4.1.12"
	},
	"dependencies": {
		"@medievalrain/emitter": "0.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/medievalrain/binance-ts.git"
	},
	"packageManager": "pnpm@10.19.0",
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
