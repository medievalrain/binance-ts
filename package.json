{
  "name": "@medievalrain/binance-ts",
  "version": "0.18.0",
  "description": "Binance API SDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medievalrain/binance-ts.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "gen:all": "NODE_OPTIONS='--localstorage-file=ls' ts-to-zod --all",
    "lint": "oxlint --type-aware",
    "format": "oxfmt --check .",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "@medievalrain/emitter": "0.8.12"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@typescript/native-preview": "7.0.0-dev.20251229.1",
    "dotenv": "17.2.3",
    "oxfmt": "0.21.0",
    "oxlint": "1.36.0",
    "oxlint-tsgolint": "0.10.0",
    "ts-to-zod": "5.1.0",
    "tsdown": "0.18.3",
    "vitest": "4.0.16",
    "zod": "4.2.1"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "packageManager": "pnpm@10.26.2",
  "access": "public"
}
